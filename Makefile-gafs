target/neo-pamgo_oomycetes.obo: mirror/gene_association.PAMGO_Oomycetes.gz
	gzip -dc mirror/gene_association.PAMGO_Oomycetes.gz | ./gaf2obo.pl -s pamgo_oomycetes -n pamgo_oomycetes > $@.tmp && mv $@.tmp $@


